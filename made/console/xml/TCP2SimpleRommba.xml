<?xml version='1.0' encoding='UTF-8'?>
<idumo ver='0.1' type='console' name='TodaysWeather'
 package='com.hixi_hyi.idumo.console.auto.app'>
<items>
<item class='ReceiverTCPProvider' name='provider' init='10000'/>
<item class='SimpleRommbaCommandHandler' name='converter'/>
<item class='SerialSendReceiptor' name='serial' init='/dev/tty.ESD200v117-0CC2EC-Gener'/>
</items>
<connections>
<connect src='provider' sink='converter'/>
<connect src='converter' sink='serial'/>
</connections>
<executions>
<loop count='-1'/>
<sleep time='1000'/>
</executions>
</idumo>
